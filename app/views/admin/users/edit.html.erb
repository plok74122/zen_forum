<% if @user.errors.any? %>
    <ul>
      <% @user.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
    </ul>
<% end %>


<%= form_for @user, :url => admin_user_path do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :profile %>
<%= f.text_area :profile %>
<%= f.select(:role, [['admin',"admin"],['normal',"normal"]]) %>
<%= f.submit %>

<% end %>